#include <iostream>
using namespace std;

int main(){
    int arr[3]={1,0,3};
    int size=sizeof(arr)/sizeof(arr[0]);
    int array[size];

    for(int i=0;i<size;i++){
        for(int j=i+1;j<size;j++){
            if(arr[i]>arr[j]){
            int temp=arr[i];
            arr[i]=arr[j];
            arr[j]=temp;
            }
        }
    }
    for(int i=0;i<size;i++){
        cout<<arr[i]<<endl;
        if(arr[i]!=i){
            cout<<i<<endl;
            break;
        }
    }
    
}
